{
  "manifestVersion": 1,
  "id": "worklog-extension",
  "publisher": "T3rr0rS0ck3",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "version": "0.0.8",
  "name": "Worklog Extension",
  "description": "Logge Arbeitszeit in Minuten/Stunden mit automatischer History.",
  "categories": [
    "Azure Boards"
  ],
  "scopes": [
    "vso.work_write"
  ],
  "files": [
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "overview.md"
    },
    {
      "path": "images",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "worklog-control",
      "type": "ms.vss-work-web.work-item-form-control",
      "targets": [
        "ms.vss-work-web.work-item-form"
      ],
      "properties": {
        "name": "Worklog Control",
        "uri": "dist/index.html",
        "height": 140
      }
    }
  ],
  "branding": {
    "color": "rgb(0, 120, 212)",
    "theme": "dark",
    "icon": "images/icon.png"
  }
}